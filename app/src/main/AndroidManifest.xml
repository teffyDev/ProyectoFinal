<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoFinal"
        tools:targetApi="31">
        <activity
            android:name=".ActivityPerfil"
            android:exported="false"
            android:label="@string/title_activity_perfil"
            android:theme="@style/Theme.ProyectoFinal" />
        <activity
            android:name=".ActivityRegistrar"
            android:exported="false"
            android:label="@string/title_activity_registrar"
            android:theme="@style/Theme.ProyectoFinal" />
        <activity
            android:name=".ActivityRecuperar"
            android:exported="false"
            android:label="@string/title_activity_recuperar"
            android:theme="@style/Theme.ProyectoFinal" />
        <activity
            android:name=".RecuperaActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ProyectoFinal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>